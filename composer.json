{
    "name": "symplify/easy-testing",
    "description": "Testing made easy",
    "license": "MIT",
    "type": "symfony-bundle",
    "bin": [
        "bin/easy-testing"
    ],
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "symfony/finder": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symplify/package-builder": "^11.0",
        "symplify/smart-file-system": "^11.0",
        "symplify/symplify-kernel": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\EasyTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\EasyTesting\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.4-dev"
        }
    },
    "conflict": {
        "symplify/astral": "<11.0.0",
        "symplify/composer-json-manipulator": "<11.0.0",
        "symplify/easy-coding-standard": "<11.0.0",
        "symplify/phpstan-rules": "<11.0.0",
        "symplify/rule-doc-generator-contracts": "<11.0.0",
        "symplify/php-config-printer": "<11.0.0",
        "symplify/autowire-array-parameter": "<11.0.0",
        "symplify/phpstan-extensions": "<11.0.0",
        "symplify/rule-doc-generator": "<11.0.0",
        "symplify/vendor-patches": "<11.0.0",
        "symplify/skipper": "<11.0.0",
        "symplify/symfony-static-dumper": "<11.0.0",
        "symplify/monorepo-builder": "<11.0.0",
        "symplify/config-transformer": "<11.0.0",
        "symplify/easy-ci": "<11.0.0",
        "symplify/coding-standard": "<11.0.0",
        "symplify/easy-parallel": "<11.0.0",
        "symplify/neon-config-dumper": "<11.0.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
